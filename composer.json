{
    "name": "martinoak/laravel-latte",
    "description": "Latte template engine for Laravel 10+",
    "keywords": ["laravel", "latte", "template", "view"],
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Dub",
            "email": "oakk.martin@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "latte/latte": "^3.0",
        "laravel/framework": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "MartinOak\\LaravelLatte\\": "src/LaravelLatte"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MartinOak\\LaravelLatte\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
